size(72*2,0);

real x = 25;
real y = 40;

draw((0,0)--(x+5,0)--(x+5,y+10)--(0,y+10)--cycle);
filldraw((2.5,5)--(x+2.5,5)--(x+2.5,y+5)--(2.5,y+5)--cycle,lightgray);

draw((-10,0)--(-5,0));
draw((-10,5)--(-5,5));
label("5",(-10,5/2),W);

draw((-10,y+5)--(-5,y+5));
draw((-10,y+10)--(-5,y+10));
label("5",(-10,y+(10+5)/2),W);

draw((0,-10)--(0,-5));
draw((2.5,-10)--(2.5,-5));
label("2.5",(2.5/2,-10),S);

draw((x+2.5,-10)--(x+2.5,-5));
draw((x+5,-10)--(x+5,-5));
label("2.5",(x+(5+2.5)/2,-10),S);


